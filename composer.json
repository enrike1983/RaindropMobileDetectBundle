{
    "name": "raindrop/mobiledetect-bundle",
    "type": "symfony-bundle",
    "description": "Symfony Raindrop Mobile Detect Bundle",
    "keywords": ["mobile", "mobile detect", "mobile detector", "php mobile detect", "detect mobile devices"],
    "homepage": "https://github.com/raindropdevs",
    "license": "MIT",
    "authors": [
        {
            "name": "Raindrop Dev Team",
            "homepage": "https://github.com/raindropdevs/RaindropMobileDetectBundle/graphs/contributors"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.3",
        "symfony/framework-bundle": ">=2.1.0,<2.3",
        "mobiledetect/mobiledetectlib": "dev-master",
        "symfony/yaml": "2.1.*@dev",
        "symfony/monolog-bundle": ">=2.1,<3"
    },
    "require-dev": {
        "symfony/class-loader": "2.1.x@dev",
        "twig/twig": ">=1.6.0,<2.0",
        "symfony/twig-bridge": "2.1.x@dev"
    },
    "autoload": {
        "psr-0": { "Raindrop\\MobileDetectBundle": "" }
    },
    "target-dir": "Raindrop/MobileDetectBundle"
}
